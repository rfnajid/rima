{
    "name": "rfnajid/rima",
    "description": "Mesin pencari rima pada suatu kata",
    "keywords": ["laravel", "lumen", "rima", "indonesia","semantic", "language"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=8.1.0",
        "danbovey/laravel-linkheader-paginator": "dev-master",
        "laravel/lumen-framework": "^10.0",
        "rfnajid/sukukata": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/rfnajid/laravel-linkheader-paginator"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
